    <validationRules>
        <fullName>LNE_AdPlanBudget</fullName>
        <active>true</active>
        <description>LNE Ad Plan Budget is required if Event is null</description>
        <errorConditionFormula>ISBLANK(LNEAdPlanBudget__c) &amp;&amp; ISBLANK(Event__c) &amp;&amp;
!$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorDisplayField>LNEAdPlanBudget__c</errorDisplayField>
        <errorMessage>LNE Ad Plan Budget is required if Event is not specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_AdPlanName</fullName>
        <active>true</active>
        <description>Ad Plan Name is required if Event is null</description>
        <errorConditionFormula>ISBLANK(AdPlanName__c) &amp;&amp;  ISBLANK(Event__c)&amp;&amp;
!$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorDisplayField>AdPlanName__c</errorDisplayField>
        <errorMessage>Ad Plan Name is required if Event is not specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_EventChildOffice</fullName>
        <active>true</active>
        <description>Checks to see if the user has access to the events office/division. If not, user can not create child records related to the event.</description>
        <errorConditionFormula>!CONTAINS( &apos;,&apos; + $User.OfficeAccess__c + &apos;,&apos;,
&apos;,&apos; + Event__r.Office__r.OfficeID__c + &apos;,&apos;)
&amp;&amp; Event__c &lt;&gt; NULL
&amp;&amp; !$Setup.GeneralSettings__c.GlobalDataAdministrator__c
&amp;&amp; !$Setup.GeneralSettings__c.BypassAutomation__c &amp;&amp;

/* Financial Calculator is running and making updates */
!$Setup.GeneralSettings__c.BypassPBFlow__c</errorConditionFormula>
        <errorMessage>You cannot create child records of the event because you do not have authorized access to the events office/division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_ExemptionReasonBlank</fullName>
        <active>true</active>
        <description>If Ad Plan Not Required is not checked and was previously not checked, Exemption Reason must be blank.</description>
        <errorConditionFormula>AdPlanNotRequired__c = FALSE &amp;&amp; PRIORVALUE(AdPlanNotRequired__c) = FALSE &amp;&amp; NOT(ISBLANK(ExemptionReason__c)) &amp;&amp; !$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorDisplayField>AdPlanNotRequired__c</errorDisplayField>
        <errorMessage>Ad Plan Not Required should be checked or Exemption Reason must be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_ExemptionReasonNotBlank</fullName>
        <active>true</active>
        <description>If Ad Plan Not Required is checked, then Exemption Reason can&apos;t be blank.</description>
        <errorConditionFormula>AdPlanNotRequired__c = TRUE &amp;&amp;  ISBLANK(ExemptionReason__c) &amp;&amp;
!$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorDisplayField>ExemptionReason__c</errorDisplayField>
        <errorMessage>If Ad Plan Not Required is checked, then Exemption Reason is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_StandAloneDate</fullName>
        <active>true</active>
        <description>If ad plan is standalone (not associated with an event), standalone date is required.</description>
        <errorConditionFormula>ISBLANK(Event__c) &amp;&amp; ISBLANK(StandaloneDate__c)&amp;&amp;
!$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorDisplayField>StandaloneDate__c</errorDisplayField>
        <errorMessage>If ad plan is standalone, standalone date is required.</errorMessage>
    </validationRules>

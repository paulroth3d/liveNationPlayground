    <fields>
        <fullName>ArtistEarningsPotential__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(DealType__c, &apos;Flat&apos;),
/* TRUE */
TotalGuaranteesandOverhead__c,

/* FALSE - other Deal Types */
IF(GuaranteeAmount__c + InclusiveArtistExpensesPerDealCalc__c + BonusPotentialGuarantee__c =
 MAX(
  /* LEFT SIDE of MAX */
  GuaranteeAmount__c + InclusiveArtistExpensesPerDealCalc__c + BonusPotentialGuarantee__c,

  /* RIGHT SIDE of MAX */
  CASE(TEXT(DealType__c),
   &apos;Percentage of Gross&apos;, (Event__r.NetGrossPotential__c * SplitBackendPercent__c),
   &apos;Percentage of Net&apos;, (Event__r.NetPotPotential__c * SplitBackendPercent__c),
   &apos;Promoter Profit&apos;, (Event__r.PotentialToBeSharedAmount__c * SplitBackendPercent__c),
   &apos;Gross Split Point&apos;, (InclusiveArtistExpensesPerDealCalc__c + (Event__r.PotentialToBeSharedAmount__c * SplitBackendPercent__c)),
  /* CASE() DEFAULT - and applies to all Retro/Step Up deal types */
   BackendPotential__c)

 /* END MAX */
 ),

/* TRUE - Event did not go into percentages - guarantees paid out */
TotalGuaranteesandOverhead__c,

/* FALSE - Event went into percentages - artist participation/overage paid out */
CASE(TEXT(DealType__c),
 &apos;Percentage of Gross&apos;, (Event__r.NetGrossPotential__c * SplitBackendPercent__c),
 &apos;Percentage of Net&apos;, (Event__r.NetPotPotential__c * SplitBackendPercent__c),
 &apos;Promoter Profit&apos;, (Event__r.PotentialToBeSharedAmount__c * SplitBackendPercent__c),
 &apos;Gross Split Point&apos;, (InclusiveArtistExpensesPerDealCalc__c + (Event__r.PotentialToBeSharedAmount__c * SplitBackendPercent__c)),
/* CASE() DEFAULT - and applies to all Retro/Step Up deal types */
 BackendPotential__c)

 /* END IFs */
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Artist Earnings Potential</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>

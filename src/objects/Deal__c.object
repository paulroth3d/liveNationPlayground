    <validationRules>
        <fullName>LNE_BackEndPercent</fullName>
        <active>true</active>
        <description>BackEnd % required if Deal Type is Alternative Promoter Profit Deal, Gross Split Point, Percentage of Gross, Percentage of Net or Promoter Profit</description>
        <errorConditionFormula>OR(
ISPICKVAL(DealType__c, &quot;Alternative Promoter Profit Deal&quot;),
ISPICKVAL(DealType__c, &quot;Gross Split Point&quot;),
ISPICKVAL(DealType__c, &quot;Percentage of Gross&quot;),
ISPICKVAL(DealType__c, &quot;Percentage of Net&quot;),
ISPICKVAL(DealType__c, &quot;Promoter Profit&quot;)) &amp;&amp; ISBLANK(BackendPercent__c) &amp;&amp; !$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorDisplayField>BackendPercent__c</errorDisplayField>
        <errorMessage>BackEnd % required if Deal Type is Alternative Promoter Profit Deal, Gross Split Point, Percentage of Gross, Percentage of Net or Promoter Profit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_EventChildOffice</fullName>
        <active>true</active>
        <description>Checks to see if the user has access to the events office/division. If not, user can not create child records related to the event.</description>
        <errorConditionFormula>!CONTAINS( &apos;,&apos; + $User.OfficeAccess__c + &apos;,&apos;,
&apos;,&apos; + Event__r.Office__r.OfficeID__c + &apos;,&apos;)
&amp;&amp; !$Setup.GeneralSettings__c.GlobalDataAdministrator__c
&amp;&amp; !$Setup.GeneralSettings__c.BypassAutomation__c &amp;&amp;

/* Financial Calculator is running and making updates */
!$Setup.GeneralSettings__c.BypassPBFlow__c</errorConditionFormula>
        <errorMessage>You cannot create child records of the event because you do not have authorized access to the events office/division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_OneRetroStepUpMustStartWithZero</fullName>
        <active>true</active>
        <description>At least one retro/step up child record must start with zero</description>
        <errorConditionFormula>RetroStepUpStartAtZero__c  = false</errorConditionFormula>
        <errorMessage>At least one of the Artist Retro/Step Up detail records for this Deal must have a starting range of zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_OtherTalentDescription</fullName>
        <active>true</active>
        <description>Other Talent Description required if Other Talent Amount &gt; 0</description>
        <errorConditionFormula>ISBLANK(OtherTalentDescription__c) &amp;&amp;  OtherTalentAmount__c &gt; 0 &amp;&amp; !$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorDisplayField>OtherTalentDescription__c</errorDisplayField>
        <errorMessage>Other Talent Description required if Other Talent Amount &gt; 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_PrimaryHeadlinerArtist</fullName>
        <active>true</active>
        <description>If &apos;Duplicate Primary Headliner Artist?&apos; is checked, then user can&apos;t save the deal record.</description>
        <errorConditionFormula>DuplicatePrimaryHeadlinerArtist__c = TRUE &amp;&amp;
!$Setup.GeneralSettings__c.BypassAutomation__c</errorConditionFormula>
        <errorMessage>You can only select one Primary Headliner per Event.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>

        <fullName>ArtistWalkoutPotential__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   ISPICKVAL(DealType__c, &apos;Flat&apos;) ||
   ISPICKVAL(DealType__c, &apos;Percentage of Gross&apos;) ||
   ISPICKVAL(DealType__c, &apos;Percentage of Net&apos;) ||
   ISPICKVAL(DealType__c, &apos;Retro Gross Deal&apos;) ||
   ISPICKVAL(DealType__c, &apos;Retro Net Deal&apos;) ||
   ISPICKVAL(DealType__c, &apos;Step Up Gross Deal&apos;) ||
   ISPICKVAL(DealType__c, &apos;Step Up Net Deal&apos;) ||
   ISPICKVAL(DealType__c, &apos;Promoter Profit&apos;) ||
   ISPICKVAL(DealType__c, &apos;Gross Split Point&apos;),
      /* TRUE */
      ArtistEarningsPotential__c + BonusPotential__c,

      /* FALSE */
      0
/* END IF */
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Artist Walkout Potential</label>
        <precision>18</precision>

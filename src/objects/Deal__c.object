    <fields>
        <fullName>AncillaryShareAmountAtProjection__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Type__c, &apos;Internal&apos;),
   /* T */
   LNEAncillaryShareAmountAtProjection__c,
   /* F */
   IF ((ProfitShareAmtAtProjFC__c + ExpenseAdjustShareAmtAtProjFC__c + AncillaryShareAmtAtProjFC__c &lt; 0) &amp;&amp;  DownsideRisk__c = False,
      /* T */
      0,
      /* F */
      AncillaryRevenue__c *  AncillaryShareAmtAtProjFC__c
   )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ancillary Share Amount At Projection</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>

    <fields>
        <fullName>SettlementArtistEarnings__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(DealType__c, &apos;Flat&apos;),
/* TRUE */
TotalGuaranteesandOverhead__c,

/* FALSE - other Deal Types */
IF(GuaranteeAmount__c + InclusiveArtistExpensesPerDealCalc__c + SettlementBonusGuarantee__c =
 MAX(
  /* LEFT SIDE of MAX */
  GuaranteeAmount__c + InclusiveArtistExpensesPerDealCalc__c + SettlementBonusGuarantee__c,

  /* RIGHT SIDE of MAX */
  CASE(TEXT(DealType__c),
   &apos;Percentage of Gross&apos;, (Event__r.SettlementNetGross__c * SplitBackendPercent__c),
   &apos;Percentage of Net&apos;, (Event__r.SettlementNetPot__c * SplitBackendPercent__c),
   &apos;Promoter Profit&apos;, (Event__r.SettlementToBeSharedAmount__c * SplitBackendPercent__c),
   &apos;Gross Split Point&apos;, (InclusiveArtistExpensesPerDealCalc__c + (Event__r.SettlementToBeSharedAmount__c * SplitBackendPercent__c)),
  /* CASE() DEFAULT - and applies to all Retro/Step Up deal types */
   SettlementBackend__c)

 /* END MAX*/
 ),

/* TRUE - Event did not go into percentages - guarantees paid out */
TotalGuaranteesandOverhead__c,

/* FALSE - Event went into percentages - artist participation/overage paid out */
CASE(TEXT(DealType__c),
 &apos;Percentage of Gross&apos;, (Event__r.SettlementNetGross__c * SplitBackendPercent__c),
 &apos;Percentage of Net&apos;, (Event__r.SettlementNetPot__c * SplitBackendPercent__c),
 &apos;Promoter Profit&apos;, (Event__r.SettlementToBeSharedAmount__c * SplitBackendPercent__c),
 &apos;Gross Split Point&apos;, (InclusiveArtistExpensesPerDealCalc__c + (Event__r.SettlementToBeSharedAmount__c * SplitBackendPercent__c)),
  /* CASE() DEFAULT - and applies to all Retro/Step Up deal types */
 SettlementBackend__c)

/* END IFs */
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Settlement Artist Earnings</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>

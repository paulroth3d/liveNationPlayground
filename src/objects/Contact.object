    <validationRules>
        <fullName>LNE_RoleRequired</fullName>
        <active>true</active>
        <description>Require role for Vendor and Venue related Contacts</description>
        <errorConditionFormula>!$Setup.GeneralSettings__c.BypassAutomation__c &amp;&amp;
(Account.RecordType.DeveloperName = &quot;Vendor&quot; ||
Account.RecordType.DeveloperName = &quot;Venue&quot;) &amp;&amp;
ISBLANK(Roles__c)</errorConditionFormula>
        <errorDisplayField>Roles__c</errorDisplayField>
        <errorMessage>You must select at least one role for a contact at a vendor or venue.</errorMessage>
    </validationRules>

    <validationRules>
        <fullName>LNE_EventChildOffice</fullName>
        <active>true</active>
        <description>Checks to see if the user has access to the events office/division. If not, user can not create child records related to the event.</description>
        <errorConditionFormula>!$Setup.GeneralSettings__c.GlobalDataAdministrator__c &amp;&amp;
!$Setup.GeneralSettings__c.BypassAutomation__c &amp;&amp;
!CONTAINS( &apos;,&apos; + $User.OfficeAccess__c + &apos;,&apos;,
&apos;,&apos; + Event__r.Office__r.OfficeID__c + &apos;,&apos;) &amp;&amp;

/* Financial Calculator is running and making updates */
!$Setup.GeneralSettings__c.BypassPBFlow__c</errorConditionFormula>
        <errorMessage>You cannot create child records of the event because you do not have authorized access to the events office/division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_PriceRequired</fullName>
        <active>true</active>
        <description>Price required if event record type is not rental, buy sell, or hold, stage type is not settlement and is not blank.</description>
        <errorConditionFormula>!$Setup.GeneralSettings__c.GlobalDataAdministrator__c &amp;&amp;
$Profile.Name != &quot;LNE Integration&quot; &amp;&amp;
ISBLANK(Price__c) &amp;&amp;
NOT(OR(
  Event__r.RecordType.DeveloperName = &quot;Rental&quot;,
  Event__r.RecordType.DeveloperName = &quot;BuySell&quot;,
  Event__r.RecordType.DeveloperName = &quot;Hold&quot;,
  ISPICKVAL(StageType__c, &quot;Settlement&quot;),
  ISPICKVAL(StageType__c, &quot;Audit&quot;),
  Event__r.UsingTicketFeed__c
))</errorConditionFormula>
        <errorDisplayField>Price__c</errorDisplayField>
        <errorMessage>Price required if event record type is not rental, buy sell, or hold and stage type is not settlement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LNE_ProjectedPaidTicketsRequired</fullName>
        <active>true</active>
        <description>Projected Paid Tickets is required if event record type is not rental, buy sell, or hold and stage type is not settlement.</description>
        <errorConditionFormula>!$Setup.GeneralSettings__c.BypassAutomation__c &amp;&amp;
$Profile.Name != &quot;LNE Integration&quot; &amp;&amp;
!Event__r.CloningInProcess__c &amp;&amp;
ISBLANK(ProjectedPaidTickets__c) &amp;&amp;
NOT(OR(
  Event__r.RecordType.DeveloperName = &quot;Rental&quot;,
  Event__r.RecordType.DeveloperName = &quot;BuySell&quot;,
  Event__r.RecordType.DeveloperName = &quot;Hold&quot;,
  ISPICKVAL(StageType__c, &quot;Settlement&quot;),
  ISPICKVAL(StageType__c, &quot;Audit&quot;)
))</errorConditionFormula>
        <errorDisplayField>ProjectedPaidTickets__c</errorDisplayField>
        <errorMessage>Projected Paid Tickets is required if event record type is not rental, buy sell, or hold and stage type is not settlement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Projected_Paid_Greater_Than_Capacity</fullName>
        <active>true</active>
        <description>Projected Paid Tickets is entered as greater than Sellable Capacity</description>
        <errorConditionFormula>!$Setup.GeneralSettings__c.BypassAutomation__c &amp;&amp;
$Profile.Name != &quot;LNE Integration&quot; &amp;&amp;
SellableCapacity__c &gt; 0 &amp;&amp;
ProjectedPaidTickets__c &gt; SellableCapacity__c</errorConditionFormula>
        <errorMessage>Projected Paid Tickets may not be greater than Sellable Capacity. Reduce the Projected Paid Tickets entered.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
